                        return (
                                top >= window.pageYOffset &&
                                (top + height) <= (window.pageYOffset + window.innerHeight)
                                );
                    }
                    angular.element($window).bind('scroll', function() {
                        scope.listCnt = attrs.searchListCount;
                        var status = loadMore(element[0]);
