	                            var to = list_parent.offsetLeft + list_elem_width;
	                            var rv = animateRight(list_parent, from, to, prev_limit);
	                            if (rv !== false && bullet_flag === 1) {
	                                var timeout = (Math.ceil(list_elem_width / move_unit) * move_timeout) + 50;
