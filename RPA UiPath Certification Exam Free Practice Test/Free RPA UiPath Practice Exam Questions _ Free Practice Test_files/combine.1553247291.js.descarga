	                                        }else if(list_parent.children[i].className.indexOf("hidden_class")==-1 &&
	                                                list_parent.children[i].className.indexOf("empty_li")==-1 &&
	                                                list_parent.children[i].className.indexOf("download_schedule_div")!=-1){
