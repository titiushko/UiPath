	                                    copiedObjs[hiddenEleCount - (i + 1)] = list_parent.children[lastEleIndex];
	                                    clonedObjs[hiddenEleCount - (i + 1)] = copiedObjs[hiddenEleCount - (i + 1)].cloneNode(true);
