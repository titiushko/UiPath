WebTracking.eventReq.setType = function(value) {
    if (typeof value != 'undefined' && value != '') {
        if (value === 'true') {
            value = true;
        } else if (value === 'false') {
            value = false;
        } else if (!isNaN(value)) {
            value = Number(value) || 0;
        } else if (this.IsJsonString(value)) {
            value = JSON.parse(value) || {};
        } else {
            value = value || "";
        }
    } else {
        value = "";
    }
    return value;
}
