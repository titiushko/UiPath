                            $scope.queryStatusMsg = "Some error occurred. Please try again later.";
                            $scope.queryStatusClass = "error_msg";
                        }
                    }
                });
            
	}
	
	
	$scope.initContactPageForm = function(){
		
		
	}
	
	$scope.agreeQueryForm = function($event){
